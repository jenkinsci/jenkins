<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <st:documentation>
    Section header in the form table.

    <st:attribute name="title" use="required">
      The section header text.
      If null is given, the entire &lt;f:section> tag becomes no-op.
    </st:attribute>
    <st:attribute name="name">
      Optional attribute to create a JSON object from this section.
    </st:attribute>
  </st:documentation>

  
  
  <div class="section panel panel-default">
  <st:adjunct includes="lib.form.section_" />
  <f:rowSet name="${attrs.name}">
    <j:if test="${attrs.title==null}">
      <div class="panel-section-header panel-heading no-title">
         <h4 class="panel-title"><a href="#">Add-on Options</a></h4>
      </div>
    </j:if>
    <j:if test="${attrs.title!=null}">
      <div class="panel-section-header panel-heading">
         <h4 class="panel-title"><a href="#">${title}</a></h4>
      </div>
    </j:if>
    <div class="panel-collapse"><div class="panel-body">
      <d:invokeBody />
    </div></div>
  </f:rowSet>
  </div>
</j:jelly>