<!-- Third party license acknowledgements page -->
<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:s="/lib/form">
  <d:taglib uri="licenses">
    <d:tag name="dependencies">
      <table class="jenkins-table sortable">
        <thead>
          <tr>
            <th initialSortDir="down">---Translate 917--- ${%Name}</th>
            <th>---Translate 917--- ${%Maven ID}</th>
            <th>---Translate 917--- ${%License}</th>
          </tr>
        </thead>
        <tbody>
          <d:invokeBody/>
        </tbody>
      </table>
    </d:tag>
    <d:tag name="dependency">
      <tr>
        <td>
          <a href="${attrs.url}" rel="noopener noreferrer" class="jenkins-table__link" target="_blank">${attrs.name}</a>
        </td>
        <td>
          ${attrs.groupId}:${attrs.artifactId}:${attrs.version}
        </td>
        <td>
          <d:invokeBody />
        </td>
      </tr>
    </d:tag>
    <d:tag name="description"/>
    <d:tag name="license">
      <a href="${attrs.url}" class="jenkins-table__link" rel="noopener noreferrer" target="_blank">${attrs.name}</a><br/>
    </d:tag>
  </d:taglib>
  <d:invokeBody/>
</j:jelly>
