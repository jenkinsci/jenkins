<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <f:entry title="Files to fingerprint"
    description="
     Can use wildcards like 'module/dist/**/*.zip'.
     See &lt;a href='http://ant.apache.org/manual/CoreTypes/fileset.html'>
     the @includes of Ant fileset&lt;/a> for the exact format.
     the base directory is &lt;a href='ws/'>the workspace&lt;/a>.
    ">
    <input class="setting-input" name="fingerprint_targets"
      type="text" value="${instance.targets}"/>
  </f:entry>
  <f:entry title="">
    <f:checkbox name="fingerprint_artifacts" checked="${instance.recordBuildArtifacts}" />
    Fingerprint all archived artifacts
  </f:entry>
  <f:entry title="" help="/help/tasks/fingerprint/keepDependencies.html">
    <f:checkbox name="keepDependencies" checked="${it.keepDependencies}" />
    Keep the build logs of dependencies 
  </f:entry>
</j:jelly>